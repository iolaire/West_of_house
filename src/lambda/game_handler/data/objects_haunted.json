  "leaflet": {
    "name": "cursed parchment",
    "type": "item",
    "state": {
      "is_taken": false,
      "is_read": false
    },
    "interactions": [
      {
        "verb": "READ",
        "response_original": "\"WELCOME TO ZORK! ZORK is a game of adventure, danger, and low cunning. In it you will explore some of the most amazing territory ever seen by mortals. No computer should be without one!\"",
        "response_spooky": "\"ABANDON HOPE, ALL YE WHO ENTER HERE. You have been chosen to explore the Haunted Manor and its cursed depths. Few who enter ever leave, and those who do are forever changed. The treasures you seek are cursed, the paths are treacherous, and death is only the beginning of your suffering. Welcome to your nightmare.\"",
        "state_change": {
          "is_read": true
        }
      },
      {
        "verb": "TAKE",
        "response_original": "Taken.",
        "response_spooky": "You grasp the parchment. It's cold to the touch and seems to writhe in your hands.",
        "state_change": {
          "is_taken": true
        }
      },
      {
        "verb": "DROP",
        "condition": {
          "is_taken": true
        },
        "response_original": "Dropped.",
        "response_spooky": "The parchment falls, landing with a wet sound.",
        "state_change": {
          "is_taken": false
        }
      }
    ]
  },
  "white_house": {
    "name": "haunted manor",
    "type": "scenery",
    "state": {},
    "interactions": [
      {
        "verb": "EXAMINE",
        "response_original": "The house is a beautiful colonial house which is painted white. It is clear that the owners must have been extremely wealthy.",
        "response_spooky": "The manor is a Gothic nightmare of decay and dark magic. Its walls are black with rot, and gargoyles leer from every corner. Windows glow with sickly green light, and you hear screaming from within. The owners were wealthy beyond measure, but their fortune was built on blood and suffering."
      },
      {
        "verb": "ENTER",
        "response_original": "I can't see how to get in from here.",
        "response_spooky": "The manor's entrance is sealed with iron and dark magic. You must find another way in."
      }
    ]
  },
  "front_door": {
    "name": "cursed door",
    "type": "scenery",
    "state": {
      "is_open": false,
      "is_boarded": true
    },
    "interactions": [
      {
        "verb": "OPEN",
        "response_original": "The door is boarded and you can't remove the boards.",
        "response_spooky": "The door is sealed with iron nails driven through human bones. Arcane symbols carved in blood prevent entry. The boards are fused by dark magic."
      },
      {
        "verb": "EXAMINE",
        "response_original": "The door is boarded shut.",
        "response_spooky": "The door is covered in warnings written in blood: 'Turn back', 'Death awaits', 'Abandon hope'. The boards are made from coffin lids."
      }
    ]
  },
  "kitchen_window": {
    "name": "broken window",
    "type": "scenery",
    "state": {
      "is_open": false
    },
    "interactions": [
      {
        "verb": "OPEN",
        "condition": {
          "is_open": false
        },
        "response_original": "With great effort, you open the window far enough to allow entry.",
        "response_spooky": "With great effort, you pry the window open. The glass is cracked and stained with something dark. Cold air and the smell of decay flow from within.",
