{
  "api": {
    "westofhouseapi": {
      "dependsOn": [
        {
          "attributes": [
            "Name",
            "Arn"
          ],
          "category": "function",
          "resourceName": "gameHandler"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "API Gateway"
    }
  },
  "function": {
    "gameHandler": {
      "build": true,
      "dependsOn": [
        {
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ],
          "category": "storage",
          "resourceName": "GameSessions"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "storage": {
    "GameSessions": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB"
    }
  }
}